os.Remove("./api.db")

	db, err := sql.Open("sqlite3", "./api.db")
	if err != nil {
		log.Fatal(err)
	}
	defer db.Close()

	sqlStmt := `
	create table api (id int(11) not null primary key, cpf, secret, name, balance, data Date);
	delete from api;`

	_, err = db.Exec(sqlStmt)
	if err != nil {
		log.Printf("%q: %s\n", err, sqlStmt)
		return
	}